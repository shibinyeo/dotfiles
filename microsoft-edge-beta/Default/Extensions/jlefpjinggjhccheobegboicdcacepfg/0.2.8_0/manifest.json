{
   "background": {
      "scripts": [ "scripts/jquery.min.js", "scripts/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "images/icon-16.png",
         "32": "images/icon-32.png"
      },
      "default_title": "ColorFish Color Picker"
   },
   "description": "Open-Source Color Picker for Websites and Desktop Apps",
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "64": "images/icon-64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "ColorFish Color Picker",
   "options_page": "options.html",
   "permissions": [ "contextMenus", "notifications", "nativeMessaging", "activeTab", "storage" ],
   "short_name": "Color Picker",
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "0.2.8",
   "web_accessible_resources": [ "dialog.html", "config/config.json", "images/gear.png", "images/close.png", "images/trash.png", "images/colorfish_cursor.png" ]
}
